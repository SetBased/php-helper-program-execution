{
  "name": "setbased/helper-program-execution",
  "description": "Program Execution Helper",
  "keywords": [
    "Program",
    "Execution"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
