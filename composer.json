{
  "name": "setbased/helper-program-execution",
  "description": "Program Execution Helper",
  "keywords": [
    "Program",
    "Execution"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "setbased/phing-extensions": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
